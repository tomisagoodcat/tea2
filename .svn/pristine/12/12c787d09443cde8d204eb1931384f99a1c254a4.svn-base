<script>
    var data = $('.effect-list .col-xs-2 > img').splice(1,3);
    var queueList = [];
    for(var i=0;i<data.length;i++){
        queueList.push(function (dom,i) {
            $(dom).delay(i*500).fadeIn()
        }(data[i],i));
    }
    $('.effect-list').queue('start-animate',queueList);
    $('.effect-list').dequeue('start-animate');

    var flowAninate = function (id,percent,duration,atom) {
        var dom = $(id),height=dom.height(),minHeight=(1-percent)*height,atom=atom||2;
        var interval = setInterval(function () {
            height-=atom;
            height<minHeight?clearInterval(interval):dom.height(height);
        },duration)
    };

    flowAninate('#row1-1',0.37,100);
    flowAninate('#row1-2',0.28,100);
    flowAninate('#row1-3',0.29,100);

    flowAninate('#row2-1',0.04,100);
    flowAninate('#row2-2',0.04,100);
    flowAninate('#row2-3',0.04,100);

    flowAninate('#row3-1',0.15,100);
    flowAninate('#row3-2',0.13,100);
    flowAninate('#row3-3',0.12,100);

    flowAninate('#row4-1',0.28,100);
    flowAninate('#row4-2',0.28,100);
    flowAninate('#row4-3',0.28,100);
</script>
